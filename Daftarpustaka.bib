file = {:C$\backslash$:/Users/admin/Downloads/Klasterisasi{\_}Menggunakan{\_}Algoritma{\_}K{\_}Mea.pdf:pdf},
issn = {2460-0741},
journal = {Jurnal Edukasi dan Penelitian Informatika},
number = {1},
pages = {62--66},
title = {{JEPIN (Jurnal Edukasi dan Penelitian Informatika) Klasterisasi Menggunakan Algoritma K-Means Clustering untuk Memprediksi Waktu Kelulusan Mahasiswa}},
volume = {5},
year = {2019}
